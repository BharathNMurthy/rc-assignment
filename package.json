{
  "name": "rc-assignment",
  "version": "1.0.0",
  "scripts": {
    "install": "cd server;npm i;cd ..;cd client;npm i",
    "build-react": "cd client && node_modules/.bin/yarn build",
    "server": "cd server; yarn start",
    "client": "cd client; yarn start",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\""
  },
  "license": "UNLICENSED",
  "devDependencies": {
    "concurrently": "^4.0.1"
  },
  "dependencies": {
    "build-react": "^1.0.1",
    "yarn": "^1.15.2"
  }
}
